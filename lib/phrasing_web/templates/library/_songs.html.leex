<section class="library--songs">
  <div class="title-bar">
    <h2 class="title">Songs</h2>
    <%= link to: Routes.song_path(@socket, :new) do %>
      <i class="fal fa-plus"></i>
    <% end %>
  </div>

  <%= if Enum.empty? @songs do %>
    <div class="empty-message">
      You don't have any songs. <br/>
      Start by adding one by clicking the plus button in the upper right hand corner.
    </div>
  <% else %>
    Some songs
  <% end %>
</section>
